# GitHub OAuth Credentials - EXAMPLE
# Copy this file and name as .env then enter your information

GITHUB_CLIENT_ID=your_client_id_here
GITHUB_CLIENT_SECRET=your_client_secret_here

# Server Configuration
PORT=3000
NODE_ENV=development

# Rate limiting (optional)
# Window in milliseconds, max requests per window
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=60

# Chrome Extension ID whitelist for redirect URI (optional)
# If set, redirect_uri must be https://<ALLOWED_EXTENSION_ID>.chromiumapp.org
# If not set, any https://*.chromiumapp.org will be allowed
ALLOWED_EXTENSION_ID=
